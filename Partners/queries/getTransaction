    AGZSName,
    LocalVCode,
    TrkType,
    DeviceName,
    Serial,
    FuelName,
    FuelShortName,
    Side,
    SideAddress,
    Nozzle,
    TrkFuelCode,
    TransNum,
    TrkTotalPriceDB,
    TrkVolumeDB,
    TrkUnitPriceDB,
    RequestTotalPriceDB,
    RequestVolumeDB,
    RequestUnitPriceDB,
    RequestField,
    State,
    iState,
    TrkTransType,
    LitersCountBeforeDB,
    MoneyCountBeforeDB,
    TransCountBefore,
    LitersCountAfterDB,
    MoneyCountAfterDB,
    TransCountAfter,
    Result,
    DateOpen,
    DateClose,
    TemperatureDB,
    PayOperationVCode,
    PayWay,
    PrePostPay,
    WUser,
    WDate,
    CUser,
    CDate,
    CHost,
    WHost,
    VCode,
    AddedForTransVCode,
    AditionalTransVCode,
    ActiveDB,
    MassDB,
    Smena,
    TrkVcode,
    CapacityVcode,
    PumpPlace,
    MoneyTakenDB,
    iPayWay,
    AutoCheckDB,
    Closed,
    FullTankDB,
    AGZS,
    FuelVCode,
    Propan
FROM [agzs].[dbo].[ADAST_TRKTransaction]
WHERE VCode = %1
